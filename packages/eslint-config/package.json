{
  "name": "@workspace/eslint-config",
  "version": "0.0.0",
  "description": "Shared ESLint config with Prettier, TypeScript, and EditorConfig",
  "license": "PROPRIETARY",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src"
